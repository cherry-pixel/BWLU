      "cell_type": "code",
      "metadata": {
        "id": "pFRCWaIo4NOq",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "#removing stopwords\n",
        "mod_title=[]\n",
        "for i in range(x.shape[0]):\n",
        "  s = x['title'][i]\n",
        "  s = s.lower()\n",
        "  s = s.split()\n",
        "  sentence = [stemmer.stem(w) for w in s if w not in set(stopwords.words('english'))]\n",
        "  sentence = \" \".join(sentence)\n",
        "  mod_title.append(sentence)\n"
      ],
      "execution_count": 45,
      "outputs": []
    },
    {
